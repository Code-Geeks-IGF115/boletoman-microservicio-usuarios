{% extends 'base.html.twig' %}

{% block title %}MetodoPago index{% endblock %}

{% block body %}
    <h1>MetodoPago index</h1>

    <table class="table">
        <thead>
            <tr>
                <th>Id</th>
                <th>NumeroTarjeta</th>
                <th>FechaVencimiento</th>
                <th>CodigoSeguridad</th>
                <th>NombrePropietario</th>
                <th>actions</th>
            </tr>
        </thead>
        <tbody>
        {% for metodo_pago in metodo_pagos %}
            <tr>
                <td>{{ metodo_pago.id }}</td>
                <td>{{ metodo_pago.numeroTarjeta }}</td>
                <td>{{ metodo_pago.fechaVencimiento }}</td>
                <td>{{ metodo_pago.codigoSeguridad }}</td>
                <td>{{ metodo_pago.nombrePropietario }}</td>
                <td>
                    <a href="{{ path('app_metodo_pago_show', {'id': metodo_pago.id}) }}">show</a>
                    <a href="{{ path('app_metodo_pago_edit', {'id': metodo_pago.id}) }}">edit</a>
                </td>
            </tr>
        {% else %}
            <tr>
                <td colspan="6">no records found</td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

    <a href="{{ path('app_metodo_pago_new') }}">Create new</a>
{% endblock %}
